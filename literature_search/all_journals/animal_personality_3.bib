Address = {{1160 BATTERY STREET, STE 100, SAN FRANCISCO, CA 94111 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{McEvoy, J (Reprint Author), Univ Tasmania, Sch Biol Sci, Hobart, Tas, Australia.
   McEvoy, Jo; While, Geoffrey M.; Jones, Susan M.; Wapstra, Erik, Univ Tasmania, Sch Biol Sci, Hobart, Tas, Australia.
   While, Geoffrey M., Univ Oxford, Dept Zool, Edward Grey Inst, Oxford, England.}},
DOI = {{10.1371/journal.pone.0125015}},
