@article{ ISI:000394450000004,
Author = {Michalko, Radek and Kosulic, Ondrej and Rezucha, Radomil},
Title = {{Link between Aggressiveness and Shyness in the Spider Philodromus
   albidus (Araneae, Philodromidae): State Dependency over Stability}},
Journal = {{JOURNAL OF INSECT BEHAVIOR}},
Year = {{2017}},
Volume = {{30}},
Number = {{1}},
Pages = {{48-59}},
Month = {{JAN}},
Abstract = {{Behavioral syndromes, seen as correlations among two or more
   functionally different behaviors, are well documented in many different
   animal taxa. They may not be present automatically, however, and their
   consistency within populations and individuals also varies among
   studies. Here, we studied a behavioral syndrome comprising foraging
   aggressiveness and boldness/shyness and its time consistency in the
   cursorial spider Philodromus albidus. We measured foraging
   aggressiveness as the number of prey killed per 2-hour period.
   Boldness/shyness was assessed as a latency of initiating exploration in
   a novel environment. We found the presence of the behavioral syndrome in
   P. albidus, as bold individuals were also more aggressive than were shy
   individuals. The syndrome was consistent through time within the
   population but not consistent for individuals. We further discuss the
   possible causes of the pattern.}},
