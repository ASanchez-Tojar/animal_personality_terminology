@article{ ISI:000312454200006,
Author = {Cezilly, Frank and Favrat, Adrien and Perrot-Minnot, Marie-Jeanne},
Title = {{Multidimensionality in parasite-induced phenotypic alterations: ultimate
   versus proximate aspects}},
Journal = {{JOURNAL OF EXPERIMENTAL BIOLOGY}},
Year = {{2013}},
Volume = {{216}},
Number = {{1, SI}},
Pages = {{27-35}},
Month = {{JAN}},
Abstract = {{In most cases, parasites alter more than one dimension in their host
   phenotype. Although multidimensionality in parasite-induced phenotypic
   alterations (PIPAs) seems to be the rule, it has started to be addressed
   only recently. Here, we critically review some of the problems
   associated with the definition, quantification and interpretation of
   multidimensionality in PIPAs. In particular, we confront ultimate and
   proximate accounts, and evaluate their own limitations. We end up by
   introducing several suggestions for the development of future research,
   including some practical guidelines for the quantitative analysis of
   multidimensionality in PIPAs.}},
