Author = {Huntingford, F. A. and Andrew, G. and Mackenzie, S. and Morera, D. and
   Coyle, S. M. and Pilarczyk, M. and Kadri, S.},
Title = {{Coping strategies in a strongly schooling fish, the common carp Cyprinus
   carpio}},
