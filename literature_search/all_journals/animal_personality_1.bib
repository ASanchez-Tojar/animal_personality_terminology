@article{ ISI:000338412600001,
Author = {Ingley, Spencer J. and Johnson, Jerald B.},
Title = {{Animal personality as a driver of reproductive isolation}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
Year = {{2014}},
Volume = {{29}},
Number = {{7}},
Pages = {{369-371}},
Month = {{JUL}},
Abstract = {{Although interest in the ecological and evolutionary implications of
   animal personality continues to grow, the role that personality plays in
   speciation has received only modest attention. Here we explore links
   between personality and speciation, and offer a framework for addressing
   some of this field's most interesting questions.}},
Publisher = {{ELSEVIER SCIENCE LONDON}},
