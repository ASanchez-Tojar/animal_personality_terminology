@article{ ISI:000284208600014,
Author = {Barber, Iain and Dingemanse, Niels J.},
Title = {{Parasitism and the evolutionary ecology of animal personality}},
Journal = {{PHILOSOPHICAL TRANSACTIONS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
Year = {{2010}},
Volume = {{365}},
Number = {{1560}},
Pages = {{4077-4088}},
Month = {{DEC 27}},
Abstract = {{The ecological factors responsible for the evolution of individual
   differences in animal personality (consistent individual differences in
   the same behaviour across time and contexts) are currently the subject
   of intense debate. A limited number of ecological factors have been
   investigated to date, with most attention focusing on the roles of
   resource competition and predation. We suggest here that parasitism may
   play a potentially important, but largely overlooked, role in the
   evolution of animal personalities. We identify two major routes by which
   parasites might influence the evolution of animal personality. First,
   because the risk of acquiring parasites can be influenced by an
   individual's behavioural type, local parasite regimes may impose
   selection on personality traits and behavioural syndromes (correlations
   between personality traits). Second, because parasite infections have
   consequences for aspects of host `state', parasites might induce the
   evolution of individual differences in certain types of host behaviour
   in populations with endemic infections. Also, because infection often
   leads to specific changes in axes of personality, parasite infections
   have the potential to decouple behavioural syndromes. Host-parasite
   systems therefore provide researchers with valuable tools to study
   personality variation and behavioural syndromes from a proximate and
   ultimate perspective.}},
